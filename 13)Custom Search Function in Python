def search(list, n):
    i = 0

    while i< len(list):
        if list[i] == n:
            return True
        i = i+1
        
    return False

list = [5,6,7,8,9]
n=9

if search(list,n):
    print("Found at")
else:
    print("not found")

        

def search(list, n):
    i = 0

    while i< len(list):
        if list[i] == n:
            globals()['pos'] = i
            return True
        i = i+1
        
    return False

list = [5,6,7,8,9]
n=9

